{
  "name": "byjg/swagger-test",
  "description": "A set of tools for test your REST calls based on the swagger documentation using PHPUnit",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "João Gilberto Magalhães",
      "email": "joao@byjg.com.br"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "6.*",
    "byjg/uri": "1.0.*",
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "ByJG\\Swagger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
